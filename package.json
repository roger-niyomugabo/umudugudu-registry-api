{
  "name": "service",
  "version": "0.0.1",
  "description": "Visit Record API for village visit record keeping",
  "main": "src/index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "type": "tsc --noEmit",
    "type-watch": "yarn run type -- --watch --preserveWatchOutput",
    "start-nodemon": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "start-dev": "run-p -l type-watch start-nodemon",
    "test": "NODE_ENV=test npx jest --runInBand --forceExit --detectOpenHandles",
    "clean": "rm -rf dist",
    "build-dev": "yarn run clean && babel src -d dist --ignore=\"./src/**/*.spec.ts\" --extensions \".ts\"",
    "build": "run-p -l type build-dev",
    "start": "node dist/index.js",
    "lint": "eslint \"./**/*.{ts,tsx}\" --cache",
    "lint:fix": "npm run lint -- --fix",
    "populate": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "typescript",
    "babel",
    "boilerplate",
    "express",
    "scaffolding"
  ],
  "author": "visit",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-sonarjs": "^0.15.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.40.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-fetch": "2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "winston": "^3.8.1"
  }
}
